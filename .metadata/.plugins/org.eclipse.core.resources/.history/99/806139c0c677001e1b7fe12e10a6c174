<script>
$(function() {
    // 모든 h2 태그를 선택하여 배열로 저장합니다.
    var $h2Elements = $("h2");
    
    // 초기에 모든 h2 태그의 크기를 설정합니다.
    $h2Elements.css("font-size", "36px");

    // 1초(1000 밀리초)마다 h2 태그 배열을 순회하면서 글꼴 크기를 작게 설정합니다.
    var index = 0;
    var interval = setInterval(function() {
        if (index < $h2Elements.length) {
            $h2Elements.eq(index).css("font-size", "25px");
            index++;
        } else {
            clearInterval(interval); // 모든 h2 태그를 처리하면 setInterval을 멈춥니다.
        }
    }, 100); // 1초마다 실행
});
</script>


